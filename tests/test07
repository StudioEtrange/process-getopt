test_body() {
    source ../src/process-getopt
    ASDA_func() { [ "${1:-""}" ] && ASDA="yes"; }
    add_opt "ASDA" a a
    add_opt "ASDA" a a
    return $?
}

test_setup() {
    TEST_TITLE="add_opt on existing option"
    TEST_ARGS=""
    TEST_IGNORE_STDOUT=
    TEST_IGNORE_STDERR=
    TEST_IGNORE_VALUE=
    TEST_EXP_VAL=1
    cat > $TEST_EXP_STDOUT <<"EOF"
EOF
    cat > $TEST_EXP_STDERR <<"EOF"
test_body: process-getopt: option name "ASDA" is already in use
EOF
}
