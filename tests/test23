test_body() {
    (cd ..; sh ./example-script $@)
    return $?
}

test_setup() {
    TEST_ARGS="-s -t token --long value -c"
    TEST_TITLE="tiny $TEST_ARGS"
    TEST_IGNORE_STDOUT=
    TEST_IGNORE_STDERR=
    TEST_IGNORE_VALUE=
    TEST_EXP_VAL=0
    cat > $TEST_EXP_STDOUT <<"EOF"
example-script: got a SLOT option = "-t"
Remaining args:
"token"
"--long"
"value"
"-c"
example-script operating on these: token --long value -c
EOF
    cat > $TEST_EXP_STDERR <<"EOF"
EOF
}
