test_body() {
    set -eu
    source ../src/process-getopt
    return $?
}

test_setup() {
    TEST_TITLE="Source process-getopt with -eu flags"
    TEST_ARGS=""
    TEST_IGNORE_STDOUT=
    TEST_IGNORE_STDERR=
    TEST_IGNORE_VALUE=
    TEST_EXP_VAL=0
    cat > $TEST_EXP_STDOUT <<EOF
EOF
    cat > $TEST_EXP_STDERR <<EOF
EOF
}
